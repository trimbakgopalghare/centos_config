        <schema>
            <key>/schemas/desktop/ibus/engine/Chewing/forceLowercaseEnglish</key>
            <applyto>/desktop/ibus/engine/Chewing/forceLowercaseEnglish</applyto>
            <owner>ibus-chewing</owner>
            <type>bool</type>
            <default>0</default>
            <locale name="C">
                <short>Force lowercase in En mode</short>
                <long>
                    Ignore CapsLock status and input lowercase by default.
                    It is handy if you wish to enter lowercase by default. Uppercase can still be inputted with Shift.
                </long>
            </locale>
            <locale name="zh_CN">
                <short>Force lowercase in En mode</short>
                <long>
