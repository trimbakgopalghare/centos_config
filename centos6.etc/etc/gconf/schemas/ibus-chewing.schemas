            <applyto>/desktop/ibus/engine/Chewing/KBType</applyto>
            <owner>ibus-chewing</owner>
            <type>string</type>
            <default>default</default>
            <locale name="C">
                <short>Keyboard Type</short>
                <long>
                    Select Zhuyin keyboard layout.
                </long>
            </locale>
            <locale name="zh_CN">
                <short>Keyboard Type</short>
                <long>
                    Select Zhuyin keyboard layout.
                </long>
            </locale>
            <locale name="zh_TW">
