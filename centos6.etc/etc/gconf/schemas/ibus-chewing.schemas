            </locale>
        </schema>
        <schema>
            <key>/schemas/desktop/ibus/engine/Chewing/selKeys</key>
            <applyto>/desktop/ibus/engine/Chewing/selKeys</applyto>
            <owner>ibus-chewing</owner>
            <type>string</type>
            <default>1234567890</default>
            <locale name="C">
                <short>Selection keys</short>
                <long>
                    Keys used to select candidate. For example "asdfghjkl;", press 'a' to select the 1st candidate, 's' for 2nd, and so on.
                </long>
            </locale>
            <locale name="zh_CN">
                <short>Selection keys</short>
