        <schema>
            <key>/schemas/desktop/ibus/engine/Chewing/syncCapsLockLocal</key>
            <applyto>/desktop/ibus/engine/Chewing/syncCapsLockLocal</applyto>
            <owner>ibus-chewing</owner>
            <type>string</type>
            <default>keyboard</default>
            <locale name="C">
                <short>Sync between CapsLock and IM</short>
                <long>
                    Occasionally, the CapsLock status does not match the IM, this option determines how these status be synchronized. Valid values:
                    "disable": Do nothing.
                    "keyboard": IM status follows keyboard status.
                    "IM": Keyboard status follows IM status.
                </long>
            </locale>
            <locale name="zh_CN">
                <short>Sync between CapsLock and IM</short>
                <long>
                    Occasionally, the CapsLock status does not match the IM, this option determines how these status be synchronized. Valid values:
                    "disable": Do nothing.
                    "keyboard": IM status follows keyboard status.
                    "IM": Keyboard status follows IM status.
                </long>
            </locale>
            <locale name="zh_TW">
                <short>Sync between CapsLock and IM</short>
                <long>
                    Occasionally, the CapsLock status does not match the IM, this option determines how these status be synchronized. Valid values:
                    "disable": Do nothing.
                    "keyboard": IM status follows keyboard status.
                    "IM": Keyboard status follows IM status.
                </long>
            </locale>
        </schema>
