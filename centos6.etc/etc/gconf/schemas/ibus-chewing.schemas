        <schema>
            <key>/schemas/desktop/ibus/engine/Chewing/inputStyle</key>
            <applyto>/desktop/ibus/engine/Chewing/inputStyle</applyto>
            <owner>ibus-chewing</owner>
            <type>string</type>
            <default>in candidate window</default>
            <locale name="C">
                <short>Input Style</short>
                <long>
                    Input style determines where the preedit strings be shown and edited.
                    "in application window": Preedit strings are edited on the target application window.
                    "in candidate window": Preedit strings are edited on the candidate selection window.
                </long>
            </locale>
            <locale name="zh_CN">
